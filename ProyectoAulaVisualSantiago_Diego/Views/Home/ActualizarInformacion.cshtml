
@{
    ViewBag.Title = "Actualizar informacion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<html>
<head>
    <title>Cambiar Eps</title>
    <link rel="stylesheet" href="~/Content/sapo.css" />
</head>
<body>
    <div>
        <h3>Cambiar: </h3>

        @using (Html.BeginForm("IngresarInformacion", "Home", FormMethod.Post))
        {
            <form action="Paciente/Paciente" method="post">
                <table>
                    <tr>
                        <td><label>Ingrese la identificación</label></td>
                        <td><input type="text" name="id" pattern="[0-9]+" oninput="this.value=this.value.replace(/[^0-9]/g,'');" value=" " /></td>
                    </tr>
                    <tr>
                        <td><input type="submit" name="Cambiar" value="Cambiar" /></td>
                      
                    </tr>

                </table>
            </form>
        }

    </div>
</body>
</html>
